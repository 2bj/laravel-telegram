{
    "name": "ricardofontanelli/laravel-telegram",
    "homepage": "https://github.com/ricardofontanelli/laravel-telegram",
    "description": "A simple and lightweight Laravel 4 and 5 wrapper to interact with Telegram Bot.",
    "keywords": ["laravel", "laravel 5", "laravel 4", "telegram", "bot", "messages", "service provider", "provider", "facade"],
    "type": "library",
    "license": "BSD Style",
    "authors": [
        {
            "name": "Ricardo Fontanelli",
            "email": "ricardo.fontanelli@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "illuminate/config": "4.*|5.*",
        "illuminate/support": "4.*|5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0",
        "php-mock/php-mock-phpunit": "^1.1"
    },
    "autoload": {
        "psr-0": {"RicardoFontanelli\\LaravelTelegram\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "RicardoFontanelli\\LaravelTelegram\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RicardoFontanelli\\LaravelTelegram\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "RicardoFontanelli\\LaravelTelegram\\TelegramFacade"
            }
        }
    }
}
